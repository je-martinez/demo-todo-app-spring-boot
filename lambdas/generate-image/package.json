{
  "name": "sqs-lambda",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "local:run": "yarn build && node dist/local-run.js",
    "deploy": "./scripts/deploy-localstack.sh",
    "destroy": "./scripts/destroy-localstack.sh",
    "zip": "rm -f dist.zip && cd dist && zip -r ../dist.zip . && cd .. && zip -r dist.zip node_modules",
    "clean": "rm -rf dist dist.zip",
    "upload": "./scripts/upload-s3.sh",
    "setup-s3": "./scripts/setup-s3-and-upload.sh"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.131",
    "@types/node": "^20.11.30",
    "dotenv": "^17.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.0"
  },
  "dependencies": {
    "@google/genai": "^1.14.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "mongodb": "^6.3.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0"
  }
}
